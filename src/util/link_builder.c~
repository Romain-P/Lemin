#include <stdlib.h>
#include "lemin.h"

t_link		*add_link(t_data *data, t_node *a, t_node *b)
{
  t_link	*link;

  if (!(link = malloc(sizeof(t_link))))
    return (NULL);
  link->node_a = node_a;
  link->node_b = node_b;
  list_add(data->links, link);
  return (link);
}
